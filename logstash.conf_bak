input {
  udp {
    port => 5000
    codec => json
  }
}

filter {
  if [docker][image] == "vanger69/proxy:1.0" {
    mutate { add_field => { "type" => "nginx" } }
    grok {
      match => { "message" => "%{COMBINEDAPACHELOG}" }
    }
  }
}

output {
  elasticsearch { hosts => "http://elasticsearch:9200" }
#  stdout { codec => rubydebug }
}
